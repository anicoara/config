!---------------------------------------------------------------------------!
!                                                                           !
!                               .Xresources                                 !
!                                                                           !
!---------------------------------------------------------------------------!


! Xft settings ---------------------------------------------------------------
Xft.dpi:                        96
Xft.antialias:                  true
Xft.rgba:                       rgb
Xft.hinting:                    true
Xft.hintstyle:                  hintslight
Xft.autohint:                   0
Xft.lcdfilter:                  lcddefault

!----------------------------------------------------------------------------!
!                                                                            !
!    U R X V T - Settings                                                    !
!                                                                            !
!----------------------------------------------------------------------------!
urxvt*geometry:                  100x35
urxvt*borderLess:                false
urxvt*depth:                     32
urxvt*scrollBar_right:           true
urxvt*font:                      xft:Inconsolata:pixelsize=13:antialias=true
urxvt*cursorColor:               white
urxvt*scrollColor:               #111111
urxvt*perl-ext-common:           default,clipboard,tabbed,matcher
urxvt.scrollBar:                 off
urxvt.tabbed.tabbar-fg:          7
urxvt.tabbed.tabbar-bg:          -1
urxvt.tabbed.tab-fg:             4
urxvt.tabbed.tab-bg:             -1
urxvt*termName:                  rxvt-256color
urxvt.urlLauncher:               /opt/google/chrome/chrom
urxvt.matcher.button:            1
urxvt.urgentOnBell:              true
urxvt*transparent:               true
urxvt*shading:                   30

!-------------------------------------------------------------------------!
!                                                                         !
!    U R X V T - Colors                                                   !
!                                                                         !
!-------------------------------------------------------------------------!

urxvt.background:                     #151515
urxvt.foreground:                     #999993

! black
urxvt.color0:                         #1a1a1a
urxvt.color8:                         #404040

! red
urxvt.color1:                         #d75f5f
urxvt.color9:                         #EA8484

! green
urxvt.color2:                         #87af5f
urxvt.color10:                        #C7F09F

! yellow
urxvt.color3:                         #ffaf5f
urxvt.color11:                        #FFCC9A

! blue
urxvt.color4:                         #87afd7
urxvt.color12:                        #a5caef

! magenta
