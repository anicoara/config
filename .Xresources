! Xft settings

Xft.dpi:                        96
Xft.antialias:                  true
Xft.rgba:                       rgb
Xft.hinting:                    true
Xft.hintstyle:                  hintslight
Xft.autohint:                   0
Xft.lcdfilter:                  lcddefault

! urxvt Settings

urxvt*geometry:                  100x35
urxvt*borderLess:                false
urxvt*depth:                     32
urxvt*font:                      xft:Terminus:pixelsize=12
URxvt*cursorBlink:               true
urxvt*perl-ext-common:           default,clipboard,tabbed,matcher
urxvt.scrollBar:                 off
urxvt.tabbed.tabbar-fg:          7
urxvt.tabbed.tabbar-bg:          -1
urxvt.tabbed.tab-fg:             4
urxvt.tabbed.tab-bg:             -1
urxvt*termName:                  rxvt-256color
urxvt.urlLauncher:               /opt/google/chrome/chrome
urxvt.matcher.button:            1
urxvt.urgentOnBell:              true
urxvt*transparent:               true
urxvt*shading:                   30


! Colors

urxvt*foreground: #f2f2f2
urxvt*background: #101010
urxvt*color0: #101010
urxvt*color1: #f13a21
urxvt*color2: #93f91d
urxvt*color3: #ffd00a
urxvt*color4: #004f9e
urxvt*color5: #ec0048
urxvt*color6: #2aa7e7
urxvt*color7: #f2f2f2
urxvt*color8: #1d202f
urxvt*color9: #ff361e
urxvt*color10: #ffc005
urxvt*color11: #93ff00
urxvt*color12: #0071ff
