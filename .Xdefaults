! Xft settings

Xft.dpi:                        96
Xft.antialias:                  true
Xft.rgba:                       rgb
Xft.hinting:                    true
Xft.hintstyle:                  hintslight
Xft.autohint:                   0
Xft.lcdfilter:                  lcddefault

! urxvt Settings

urxvt*geometry:                  100x35
urxvt*borderLess:                false
urxvt*depth:                     32
urxvt*font:                      xft:Terminus:pixelsize=12
URxvt*cursorBlink:               true
urxvt*perl-ext-common:           default,clipboard,tabbed,matcher
urxvt.scrollBar:                 off
URxvt.tabbed.tabbar-fg:          10
URxvt.tabbed.tabbar-bg:          0
URxvt.tabbed.tab-fg:             12
URxvt.tabbed.tab-bg:             0
urxvt*termName:                  rxvt-256color
urxvt.urlLauncher:               /opt/google/chrome/chrome
urxvt.matcher.button:            1
urxvt.urgentOnBell:              true
urxvt*transparent:               true
urxvt*shading:                   15


! Colors

*background: #000000
*foreground: #999993

! black
*color0  : #2e3436
*color8  : #555753
! red
*color1  : #cc0000
*color9  : #ex2929
! green
*color2  : #4e9a06
*color10 : #8ae234
! yellow
*color3  : #c4a000
*color11 : #fce94f
! blue
*color4  : #3465a4
*color12 : #729fcf
! magenta
*color5  : #75507b
*color13 : #ad7fa8
! cyan
*color6  : #06989a
*color14 : #34e2e2
! white
*color7  : #d3d7cf
*color15 : #eeeeec
